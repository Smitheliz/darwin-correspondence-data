        <div type="letter">
            <div type="text">
                <opener>
                    <placeName><hi rend="print">W<hi rend="superscript">m</hi>. Clowes &amp; Sons | Stamford S<hi rend="superscript">t</hi>. S.E.</hi></placeName>
