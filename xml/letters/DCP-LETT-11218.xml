                <note xml:id="tnote.n3">1.1 be] <hi rend="italic">interl</hi> </note>
                <note xml:id="tnote.n4">1.1 much] <hi rend="italic">after</hi> ‘in some respects’ <hi rend="italic">del pencil</hi></note>
                <note xml:id="tnote.n5">1.2 at Cambridge] <hi rend="italic">interl</hi> </note>
                <note xml:id="tnote.n6">1.3 proposed] <hi rend="italic">pencil interl above del pencil</hi> ‘fixed’ </note>
